{
  "expo": {
    "name": "student-attendance-app",
    "slug": "student-attendance-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icons/bluetooth.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/bluetooth.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.studenattendanceapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/bluetooth.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "29b3136c-5a2e-4897-b73a-1da70cd7f03a"
      }
    }
  }
}
